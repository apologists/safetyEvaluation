<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.VariableMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="variableResultMap" type="org.example.entity.Variable">
        <id column="variable_id" property="variableId"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
        <result column="variable_name_cn" property="variableNameCn"/>
        <result column="variable_name_en" property="variableNameEn"/>
        <result column="project_id" property="projectId"/>
        <result column="unit_id" property="unitId"/>
        <result column="model_id" property="modelId"/>
        <result column="variable_num" property="variableNum"/>
    </resultMap>


</mapper>
