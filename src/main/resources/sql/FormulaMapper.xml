<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.FormulaMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="formulaResultMap" type="org.example.entity.Formula">
        <id column="formula_id" property="formulaId"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
        <result column="deleted" property="deleted"/>
        <result column="formula_right" property="formulaRight"/>
        <result column="formula_left" property="formulaLeft"/>
        <result column="project_id" property="projectId"/>
        <result column="unit_id" property="unitId"/>
        <result column="model_id" property="modelId"/>
    </resultMap>


</mapper>
