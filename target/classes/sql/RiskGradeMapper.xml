<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.RiskGradeMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="riskGradeResultMap" type="org.example.entity.RiskGrade">
        <id column="risk_grade_id" property="riskGradeId"/>
        <result column="updated" property="updated"/>
        <result column="created" property="created"/>
        <result column="deleted" property="deleted"/>
        <result column="risk_grade_desc" property="riskGradeDesc"/>
        <result column="risk_grade" property="riskGrade"/>
        <result column="action_ask" property="actionAsk"/>
        <result column="tolerate" property="tolerate"/>
        <result column="project_id" property="projectId"/>
        <result column="unit_id" property="unitId"/>
        <result column="frequency_id" property="frequencyId"/>
        <result column="risk_consequence_id" property="riskConsequenceId"/>
        <result column="colour" property="colour"/>
        <result column="grade_num" property="gradeNum"/>
        <result column="frequency_Num" property="frequencyNum"/>
        <result column="risk_consequence_Num" property="riskConsequenceNum"/>
    </resultMap>


</mapper>
