<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.RiskMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="riskResultMap" type="org.example.entity.Risk">
        <id column="risk_id" property="riskId"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
        <result column="deleted" property="deleted"/>
        <result column="grade" property="grade"/>
        <result column="severity" property="severity"/>
        <result column="personnel" property="personnel"/>
        <result column="property" property="property"/>
        <result column="environment" property="environment"/>
        <result column="reputation" property="reputation"/>
        <result column="laws" property="laws"/>
        <result column="lockout" property="lockout"/>
        <result column="project_id" property="projectId"/>
    </resultMap>


</mapper>
