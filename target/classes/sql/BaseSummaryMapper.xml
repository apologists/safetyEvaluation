<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.BaseSummaryMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="baseSummaryResultMap" type="org.example.entity.BaseSummary">
        <id column="base_id" property="baseId"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
        <result column="deleted" property="deleted"/>
        <result column="name" property="name"/>
        <result column="device" property="device"/>
        <result column="unit" property="unit"/>
        <result column="project_id" property="projectId"/>
        <result column="base_begin_time" property="baseBeginTime"/>
        <result column="base_end_time" property="baseEndTime"/>
        <result column="analyticalMethod" property="analyticalMethod"/>
        <result column="remark" property="remark"/>
    </resultMap>


</mapper>
